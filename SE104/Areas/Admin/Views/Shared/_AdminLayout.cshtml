@*<!DOCTYPE html>*@
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"]</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/adminassets/images/logo/favicon.png">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <!-- page css -->
    <link href="~/adminassets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet">

    <link rel="stylesheet" href="~/lib/jqueryui/themes/base/theme.css" />
    <link rel="stylesheet" href="~/lib/jqueryui/themes/base/jquery-ui.css" />
    <link rel="stylesheet" href="~/lib/jquery/dist/jquery.min.js" />
    <link rel="stylesheet" href="~/lib/jqueryui/jquery-ui.min.js" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link href="~/elfinder/css/elfinder.full.min.css" rel="stylesheet" />
    <link href="~/elfinder/css/elfinder.full.css" rel="stylesheet" />
    <link href="~/elfinder/css/theme.min.css" rel="stylesheet" />
    <link href="~/elfinder/css/theme.css" rel="stylesheet" />

    <script src="~/adminassets/js/vendors.min.js"></script>
    <script src="~/js/site.js"></script>
    <!-- page js -->
    <script src="~/adminassets/vendors/chartjs/Chart.min.js"></script>
    <script src="~/adminassets/js/pages/dashboard-e-commerce.js"></script>

    <script src="~/elfinder/js/elfinder.full.min.js"></script>
    <script src="~/elfinder/js/elfinder.full.js"></script>
    <script src="~/elfinder/js/elfinder.min.js"></script>
    <script src="~/lib/jqueryui/jquery-ui.min.js"></script>

    <link href="~/assets/fontawesome-free-6.2.1-web/css/all.css" rel="stylesheet" />
    <link href="~/assets/fontawesome-free-6.2.1-web/css/all.min.css" rel="stylesheet" />

    <script src="~/tinymce/tinymce.min.js" referrerpolicy="origin"></script>



    <script src="https://cdn.tiny.cloud/1/z1y5z6sgyrl6nu9zj9f510br8odv8lkkar971v325vzxtarm/tinymce/6/tinymce.min.js" referrerpolicy="origin"></script>


    <!-- Core css -->
    <link href="~/adminassets/css/app.min.css" rel="stylesheet">

    <link href="~/css/site.css" rel="stylesheet" />



</head>

<body>
    <div class="app">
        <div class="layout">
            <!-- Header START -->
            <div class="header">
                
                <div class="logo logo-white">
                    <a href="index.html">
                        <img src="~/adminassets/images/logo/logo-white.png" alt="Logo">
                        <img class="logo-fold" src="~/adminassets/images/logo/logo-fold-white.png" alt="Logo">
                    </a>
                </div>
               
            </div>
            <!-- Header END -->
            <!-- Side Nav START -->
            @await Html.PartialAsync("_SideNavPartialView.cshtml")
            <!-- Side Nav END -->
            <!-- Page Container START -->
            <div class="page-container">

                <!-- Content Wrapper START -->
                <div class="main-content">
                    @RenderBody()
                </div>
                <!-- Content Wrapper END -->
                <!-- Footer START -->
                <footer class="footer">
                    <div class="footer-content">
                        <p class="m-b-0">Copyright © 2019 Theme_Nate. All rights reserved.</p>
                        <span>
                            <a href="" class="text-gray m-r-15">Term &amp; Conditions</a>
                            <a href="" class="text-gray">Privacy &amp; Policy</a>
                        </span>
                    </div>
                </footer>
                <!-- Footer END -->

            </div>
            <!-- Page Container END -->
            <!-- Search Start-->
            <!-- Search End-->
            <!-- Quick View START -->
            <!-- Quick View END -->
        </div>
    </div>

    @*    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    *@
    <!-- Core Vendors JS -->
    <!-- Core JS -->
    <script src="~/adminassets/js/app.min.js"></script>

    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)


</body>

</html>
